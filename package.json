{
  "name": "swifty-frontend",
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "comp": "yarn workspace @swifty/component",
    "bank": "yarn workspace swifty-bank",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ."
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.11.4",
    "@types/react": "^18.2.48",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.2",
    "react": "^18.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.8"
  }
}
